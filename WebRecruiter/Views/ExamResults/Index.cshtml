@model IEnumerable<WebRecruiter.Models.ExamResult>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ExamSubject.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ReceivedPoints)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsAdvanced)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ExamSubject.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ReceivedPoints)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsAdvanced)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
